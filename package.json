{
  "name": "vapaee-npm-libs",
  "version": "v1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --poll=2000 -o",
    "serve": "ng serve --poll=2000 -o",
    "libs": "npm run postinstall && npm run lib-feedback && npm run lib-style && npm run lib-wallet && npm run lib-idp-scatter && npm run lib-idp-local && npm run lib-rex && npm run lib-dex",
    "lib-rex": "npm run check-node-version && ng build @vapaee/rex --prod",
    "lib-dex": "npm run check-node-version && ng build @vapaee/dex --prod",
    "lib-style": "npm run check-node-version && ng build @vapaee/style --prod",
    "lib-scatter": "npm run check-node-version && ng build @vapaee/scatter --prod",
    "lib-wallet": "npm run check-node-version && ng build @vapaee/wallet --prod",
    "lib-idp-scatter": "npm run fix-types && npm run check-node-version && ng build @vapaee/idp-scatter --prod",
    "lib-idp-local": "npm run check-node-version && ng build @vapaee/idp-local --prod",
    "lib-feedback": "npm run check-node-version && ng build @vapaee/feedback --prod",
    "publish-libs": "npm run publish-lib-feedback; npm run publish-lib-style; npm run publish-lib-wallet; npm run publish-lib-idp-scatter; npm run publish-lib-idp-local; npm run publish-lib-rex; npm run publish-lib-dex",
    "publish-lib-rex": "cd dist/vapaee/rex && npm publish --access public",
    "publish-lib-dex": "cd dist/vapaee/dex && npm publish --access public",
    "publish-lib-style": "cd dist/vapaee/style && npm publish --access public",
    "publish-lib-scatter": "cd dist/vapaee/scatter && npm publish --access public",
    "publish-lib-wallet": "cd dist/vapaee/wallet && npm publish --access public",
    "publish-lib-idp-scatter": "cd dist/vapaee/idp-scatter && npm publish --access public",
    "publish-lib-idp-local": "cd dist/vapaee/idp-local && npm publish --access public",
    "publish-lib-feedback": "cd dist/vapaee/feedback && npm publish --access public",
    "build-prod": "npm run libs",
    "build": "npm run libs",
    "prepare": "npm run check-node-version",
    "check-node-version": "scripts/npm-version.sh",
    "fix-types": "scripts/types.sh",
    "postinstall": "npm run fix-types",
    "full": "npm i; npm run libs; cd contracts/_examples; if [ ! -d 'eosio.contracts' ]; then git clone https://github.com/Telos-Foundation/eosio.contracts; fi; npm run serve",
    "save": "git add -A && git commit -m 'save'",
    "pull": "git pull; cp ../airdrop-on-telos/* ./airdrop-on-telos/ -R;",
    "prod": "git checkout prod && git merge master -m 'merge master' && npm run buildprod && git add -A && git commit -m 'update' && git push && amazon_server_console;"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "rxjs": "6.4.0",
    "tslib": "1.10.0",
    "zone.js": "0.9.1",
    "@scatterjs/core": "2.7.54",
    "@scatterjs/eosjs2": "1.5.36",
    "bignumber.js": "9.0.1",
    "bootstrap": "4.5.3",
    "core-js": "3.8.1",
    "crypto-js": "4.0.0",
    "eosjs": "21.0.3",
    "eosjs-api": "7.0.4",
    "eosjs-ecc": "4.0.7",
    "long": "4.0.0",
    "ngx-cookie-service": "10.1.1",
    "rxjs-compat": "6.6.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.29",
    "@angular-devkit/build-ng-packagr": "0.803.29",
    "@angular/cli": "8.3.29",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/node": "8.9.4",
    "@types/jasmine": "3.3.8",
    "@types/jasminewd2": "2.0.3",
    "codelyzer": "5.0.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "ng-packagr": "5.4.0",
    "protractor": "7.0.0",
    "ts-node": "7.0.0",
    "tsickle": "0.37.0",
    "tslint": "5.15.0",
    "typescript": "3.5.3",
    "@types/crypto-js": "4.0.2",
    "@types/elliptic": "6.4.13",
    "@types/long": "4.0.1"
  },
  "_devDependencies": {
    "@angular-devkit/build-angular": "0.803.29",
    "@angular-devkit/build-ng-packagr": "0.803.29"
  }
}

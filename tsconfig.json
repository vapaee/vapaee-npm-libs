{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@vapaee/feedback": [
        "dist/vapaee/feedback"
      ],
      "@vapaee/feedback/*": [
        "dist/vapaee/feedback/*"
      ],
      "@vapaee/scatter": [
        "dist/vapaee/scatter"
      ],
      "@vapaee/scatter/*": [
        "dist/vapaee/scatter/*"
      ],
      "@vapaee/dex": [
        "dist/vapaee/dex"
      ],
      "@vapaee/dex/*": [
        "dist/vapaee/dex/*"
      ],
      "@vapaee/aux": [
        "dist/vapaee/aux"
      ],
      "@vapaee/aux/*": [
        "dist/vapaee/aux/*"
      ],
      "@vapaee/style": [
        "dist/vapaee/style"
      ],
      "@vapaee/style/*": [
        "dist/vapaee/style/*"
      ],
      "@vapaee/rex": [
        "dist/vapaee/rex"
      ],
      "@vapaee/rex/*": [
        "dist/vapaee/rex/*"
      ],
      "@vapaee/wallet": [
        "dist/vapaee/wallet"
      ],
      "@vapaee/wallet/*": [
        "dist/vapaee/wallet/*"
      ],
      "@vapaee/idp-scatter": [
        "dist/vapaee/idp-scatter"
      ],
      "@vapaee/idp-scatter/*": [
        "dist/vapaee/idp-scatter/*"
      ],
      "@vapaee/idp-local": [
        "dist/vapaee/idp-local"
      ],
      "@vapaee/idp-local/*": [
        "dist/vapaee/idp-local/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "strict": true
  }
}
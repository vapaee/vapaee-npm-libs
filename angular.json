{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@vapaee/feedback": {
      "projectType": "library",
      "root": "projects/vapaee/feedback",
      "sourceRoot": "projects/vapaee/feedback/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/vapaee/feedback/tsconfig.lib.json",
            "project": "projects/vapaee/feedback/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/vapaee/feedback/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/vapaee/feedback/src/test.ts",
            "tsConfig": "projects/vapaee/feedback/tsconfig.spec.json",
            "karmaConfig": "projects/vapaee/feedback/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/vapaee/feedback/tsconfig.lib.json",
              "projects/vapaee/feedback/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
